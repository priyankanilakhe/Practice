package OptimizedSolution.Arrays;
class Solution {
    public int maxProfit(int[] prices) {
        int min= prices[prices.length-1];
        int profit=0;
        for(int i=prices.length-2;i<0;i--)
        {
            if(prices[i]<min)
            {
                min=prices[i];
                profit=min-prices[i];
            }
        }
    return profit;
    }
    
}